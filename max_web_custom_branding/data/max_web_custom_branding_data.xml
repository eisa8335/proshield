<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Company Name -->
        <record id="company_name" model="ir.config_parameter">
            <field name="key">max_web_custom_branding.company_name</field>
            <field name="value">MAXodoo</field>
        </record>
        <record id="company_url" model="ir.config_parameter">
            <field name="key">max_web_custom_branding.company_url</field>
            <field name="value">http://www.maxodoo.com</field>
        </record>
        <record id="company_name_color" model="ir.config_parameter">
            <field name="key">max_web_custom_branding.company_name_color</field>
            <field name="value">red</field>
        </record>
        <record id="system_name" model="ir.config_parameter">
            <field name="key">max_web_custom_branding.system_name</field>
            <field name="value">MAXodoo</field>
        </record>
        <record id="release_version" model="ir.config_parameter">
            <field name="key">max_web_custom_branding.release_version</field>
            <field name="value">10.0</field>
        </record>
    </data>
</odoo>
